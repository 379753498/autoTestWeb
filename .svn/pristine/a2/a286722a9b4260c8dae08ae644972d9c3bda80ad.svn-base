<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quauq.autoTest.dao.CustomerDAO">
	<resultMap type="Customer" id="resultMap">
		<id property="id" column="id" />
		<result property="name" column="name" />
		<result property="code" column="code" />
	</resultMap>
	<select id="findCustomerList" resultMap="resultMap">
		select * from t_customer
	</select>
	<insert id="insertCustomer" parameterType="Customer">
		insert into t_customer(name,code)
		values(#{name},#{code})
	</insert>
	<select id="findCustomerById" parameterType="Integer" resultMap="resultMap">
		select * from t_customer t where t.id=#{id}
	</select>
	<update id="updateCustomer" parameterType="Customer">
		update t_customer set name=#{name},code=#{code}  where id=#{id}
	</update>
	<delete id="deleteCustomer" parameterType="Integer">
		delete from t_customer where id=#{id}
	</delete>
</mapper>  

